{
    "name": "eecjimmy/app",
    "description": "",
    "type": "project",
    "authors": [
        {
            "name": "eecjimmy",
            "email": "eecjimmy@163.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-simplexml": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-openssl": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-exif": "*",
        "ext-zip": "*",
        "ext-posix": "*",
        "yiisoft/yii2": "~2.0.45",
        "yiisoft/yii2-bootstrap5": "~2.0.2",
        "yidas/yii2-composer-bower-skip": "~2.0.5",
        "codebot/phpenv": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "app\\": "./src"
        }
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "easywechat-composer/easywechat-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    ]
}